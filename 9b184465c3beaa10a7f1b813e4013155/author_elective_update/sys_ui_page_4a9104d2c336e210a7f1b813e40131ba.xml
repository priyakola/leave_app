        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide">
    <g:evaluate var="jvar_sys_id" expression="RP.getParameterValue('sysparm_sys_id')" />
    
    <g:evaluate>
    var gr = new GlideRecord('x_1800027_leve_sys_leave_request');
    if (gr.get(jvar_sys_id)) {
        gr.setWorkflow(false);
        gr.status = 'approved';
        if (gr.update()) {
            gs.addInfoMessage('Leave request approved');
        } else {
            gs.error('Failed to approve: ' + gr.getLastErrorMessage());
            gs.addErrorMessage('Failed to approve leave request');
        }
    }
    </g:evaluate>
    
    <script>
    window.onload = function() {
        window.location.href = 'x_1800027_leve_sys_leave_request.do?sys_id=${jvar_sys_id}';
    };
    </script>
    
    <p>Processing approval...</p>
</j:jelly>]]></html>
